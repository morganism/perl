<project name="VFI" basedir="../.." default="copy-ecplise-classes">

    <target name="compile">
    	<property name="module.dir" value="${basedir}"/>
		<echo>module.dir set to ${basedir}</echo>

    	<ant dir="${basedir}/../ASCERTAINUTILS/files/ant" antfile="common_build.xml" target="compile"/>
    </target>

	<target name="copy-ecplise-classes">
